{
  "name": "health-table",
  "version": "1.0.0",
  "description": "UI component which imports and shows users data from data storage ",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sarunas-k/health-table.git"
  },
  "author": "Šarūnas Kybartas",
  "homepage": "https://github.com/sarunas-k/health-table#readme",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check lint test:unit \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test:unit": "vitest --environment jsdom --root src/"
  },
  "dependencies": {
    "pinia": "^2.1.7",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.3",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.12.8",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-vue": "^9.23.0",
    "jquery": "^3.7.1",
    "jsdom": "^24.0.0",
    "msw": "^2.3.0",
    "npm-run-all2": "^6.1.2",
    "sass": "^1.76.0",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.11"
  }
}
